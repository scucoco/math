namespace http://cds.omdoc.org/logic

//   Hilbert Style Propositional logic 

     
theory PLHilbertAxioms : http://cds.omdoc.org/urtheories?LF =
	include ?PLSyntax 
	S : {φ,ψ,σ} ded (φ ⇒ (ψ ⇒ σ)) ⇒ ((φ ⇒ ψ) ⇒ (φ ⇒ σ)) 
	K : {φ,ψ} ded φ ⇒ (ψ ⇒ φ) 
	DN : {φ} ded ((φ ⇒ ¬ true) ⇒ ¬ true) ⇒ φ 


theory PLHilbertRules : http://cds.omdoc.org/urtheories?LF =
	include ?PLSyntax 
	MP : {φ, ψ} ded φ → ded φ ⇒ ψ → ded ψ 

	
theory PLHilbertPf : http://cds.omdoc.org/urtheories?LF =
	include ?PLHilbertAxioms 
	include ?PLHilbertRules 

	
