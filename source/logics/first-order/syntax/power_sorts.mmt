namespace http://latin.omdoc.org/logics/syntax

theory PowerSorts : http://cds.omdoc.org/urtheories?LF =
  include ?SFOLEQ
  equal : {S} tm S → tm S → o= [S,x,y] eq S x y# 2 == 3
  
  power : sort → sort# ℘ 1 prec 100
  in    : {S} tm S → tm ℘S → o # 2 ‍∈ 3

  filter : {A} (tm A → o) → tm ℘A  # filter 2
  
  subset : {S} tm ℘S → tm ℘ S → o  # 2 ⊆‍ 3	
  = [S : sort, A : tm ℘S, B : tm ℘S] forall ([x] ((x ‍∈ A) imp (x ‍∈ B))) 


  full : {A} tm ℘ A= [A] filter ([x] true)# id
  empty : {A} tm ℘ A= [A] filter ([x] false)  # ∅ 
  union : {S} tm ℘S → tm ℘S → tm ℘S  # 2 ∪ 3 
  inters : {S} tm ℘S → tm ℘S → tm ℘S  # 2 ∩ 3 
  
  compl : {S} tm ℘S → tm ℘S # 2 ᶜ 

  bigunion : {S} tm ℘℘S → tm ℘S  # ⋃ 2 
  biginters : {S} tm ℘℘S → tm ℘S  # ⋂ 2 

