<?xml version="1.0" encoding="UTF-8"?>
<omdoc xmlns:om="http://www.openmath.org/OpenMath" xmlns="http://omdoc.org/ns">
  <theory name="Types" base="http://latin.omdoc.org/logics/tptp" meta="http://cds.omdoc.org/urtheories?LF">
    <metadata>
      <meta property="http://purl.org/dc/terms?_?description">
        <om:OMOBJ xmlns:om="http://www.openmath.org/OpenMath">
          <om:OMSTR>
            the type system\n This is in a separate signature so that it can be shared between the first- and the higher-order languages.\n We use a Church-style formalization where terms have a unique intrinsic type.\n
          </om:OMSTR>
        </om:OMOBJ>
      </meta>
    </metadata>
    <constant name="$tType" role="type">
      <metadata>
        <meta property="http://purl.org/dc/terms?_?description">
          <om:OMOBJ xmlns:om="http://www.openmath.org/OpenMath">
            <om:OMSTR>types</om:OMSTR>
          </om:OMOBJ>
        </meta>
      </metadata>
      <type>
        <om:OMOBJ xmlns:om="http://www.openmath.org/OpenMath">
          <om:OMS base="http://cds.omdoc.org/urtheories" module="Typed" name="type"/>
        </om:OMOBJ>
      </type>
    </constant>
    <constant name="$tm" role="type">
      <metadata>
        <meta property="http://purl.org/dc/terms?_?description">
          <om:OMOBJ xmlns:om="http://www.openmath.org/OpenMath">
            <om:OMSTR>terms of a type</om:OMSTR>
          </om:OMOBJ>
        </meta>
      </metadata>
      <type>
        <om:OMOBJ xmlns:om="http://www.openmath.org/OpenMath">
          <om:OMA>
            <om:OMS position="0" base="http://cds.omdoc.org/urtheories" module="LF" name="arrow"/>
            <om:OMS position="1" base="http://latin.omdoc.org/logics/tptp" module="Types" name="$tType"/>
            <om:OMS position="2" base="http://cds.omdoc.org/urtheories" module="Typed" name="type"/>
          </om:OMA>
        </om:OMOBJ>
      </type>
      <notation>
        <text-notation name="http://latin.omdoc.org/logics/tptp?Types?$tm" precedence="2" fixity="prefix" arguments="0"/>
      </notation>
    </constant>
    <constant name="$i" role="constructor">
      <metadata>
        <meta property="http://purl.org/dc/terms?_?description">
          <om:OMOBJ xmlns:om="http://www.openmath.org/OpenMath">
            <om:OMSTR>the predefined base type</om:OMSTR>
          </om:OMOBJ>
        </meta>
      </metadata>
      <type>
        <om:OMOBJ xmlns:om="http://www.openmath.org/OpenMath">
          <om:OMS base="http://latin.omdoc.org/logics/tptp" module="Types" name="$tType"/>
        </om:OMOBJ>
      </type>
    </constant>
  </theory>
</omdoc>