<?xml version="1.0" encoding="UTF-8"?>
<omdoc xmlns:om="http://www.openmath.org/OpenMath" xmlns="http://omdoc.org/ns">
  <theory name="PFF" base="http://latin.omdoc.org/logics/tptp" meta="http://cds.omdoc.org/urtheories?LF">
    <metadata>
      <meta property="http://purl.org/dc/terms?_?description">
        <om:OMOBJ xmlns:om="http://www.openmath.org/OpenMath">
          <om:OMSTR>PFF: the extension of TFF with shallow polymorphism</om:OMSTR>
        </om:OMOBJ>
      </meta>
    </metadata>
    <import from="http://latin.omdoc.org/logics/tptp?TFF"> </import>
    <constant name="!째" role="constructor">
      <metadata>
        <meta property="http://purl.org/dc/terms?_?description">
          <om:OMOBJ xmlns:om="http://www.openmath.org/OpenMath">
            <om:OMSTR>
              quantification over types\n The TPTP syntax uses the same symbols (here: ! and ?) both for operators on terms and their analogs on types.\n Because Twelf does not support overloading, we add use \194\176 to distinguish the ones on types. The translation from TPTP to LF has to disambiguate them.\n
            </om:OMSTR>
          </om:OMOBJ>
        </meta>
      </metadata>
      <type>
        <om:OMOBJ xmlns:om="http://www.openmath.org/OpenMath">
          <om:OMA>
            <om:OMS position="0" base="http://cds.omdoc.org/urtheories" module="LF" name="arrow"/>
            <om:OMA position="1">
              <om:OMS position="1_0" base="http://cds.omdoc.org/urtheories" module="LF" name="arrow"/>
              <om:OMS position="1_1" base="http://latin.omdoc.org/logics/tptp" module="Types" name="$tType"/>
              <om:OMS position="1_2" base="http://latin.omdoc.org/logics/tptp" module="TFF" name="$form"/>
            </om:OMA>
            <om:OMS position="2" base="http://latin.omdoc.org/logics/tptp" module="TFF" name="$form"/>
          </om:OMA>
        </om:OMOBJ>
      </type>
      <notation>
        <text-notation name="http://latin.omdoc.org/logics/tptp?PFF?!째" precedence="5" fixity="prefix" arguments="0"/>
      </notation>
    </constant>
    <constant name="%3F째" role="constructor">
      <type>
        <om:OMOBJ xmlns:om="http://www.openmath.org/OpenMath">
          <om:OMA>
            <om:OMS position="0" base="http://cds.omdoc.org/urtheories" module="LF" name="arrow"/>
            <om:OMA position="1">
              <om:OMS position="1_0" base="http://cds.omdoc.org/urtheories" module="LF" name="arrow"/>
              <om:OMS position="1_1" base="http://latin.omdoc.org/logics/tptp" module="Types" name="$tType"/>
              <om:OMS position="1_2" base="http://latin.omdoc.org/logics/tptp" module="TFF" name="$form"/>
            </om:OMA>
            <om:OMS position="2" base="http://latin.omdoc.org/logics/tptp" module="TFF" name="$form"/>
          </om:OMA>
        </om:OMOBJ>
      </type>
      <notation>
        <text-notation name="http://latin.omdoc.org/logics/tptp?PFF?%3F째" precedence="5" fixity="prefix" arguments="0"/>
      </notation>
    </constant>
  </theory>
</omdoc>