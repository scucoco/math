</om:OMS>
                <om:OMA position="2_2_2">
                  <om:OMS position="2_2_2_0" base="http://cds.omdoc.org/urtheories" module="LF" name="apply"/>
                  <om:OMS position="2_2_2_1" base="http://latin.omdoc.org/foundations/type_theory/church" module="ReflectedLogic" name="reflect_o"/>
                  <om:OMV position="2_2_2_2" name="X"/>
