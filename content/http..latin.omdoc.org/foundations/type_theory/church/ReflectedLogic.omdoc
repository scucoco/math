            </om:OMBVAR>
            <om:OMA position="2">
              <om:OMS position="2_0" base="http://cds.omdoc.org/urtheories" module="LF" name="arrow"/>
              <om:OMA position="2_1">
                <om:OMS position="2_1_0" base="http://cds.omdoc.org/urtheories" module="LF" name="apply"/>
                <om:OMS 
