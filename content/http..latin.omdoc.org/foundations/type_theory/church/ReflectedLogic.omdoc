        <om:OMOBJ xmlns:om="http://www.openmath.org/OpenMath">
          <om:OMBIND>
            <om:OMS position="0" base="http://cds.omdoc.org/urtheories" module="LF" name="Pi"/>
            <om:OMBVAR>
              <om:OMV position="0" name="X">
                <type>
                  <om:OMS 
