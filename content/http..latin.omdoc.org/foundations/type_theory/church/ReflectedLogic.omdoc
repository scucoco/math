<?xml version="1.0" encoding="UTF-8"?>
<omdoc xmlns:om="http://www.openmath.org/OpenMath" xmlns="http://omdoc.org/ns">
  <theory name="ReflectedLogic" base="http://latin.omdoc.org/foundations/type_theory/church" meta="http://cds.omdoc.org/urtheories?LF">
    <metadata>
      <meta property="http://purl.org/dc/terms?_?description">
        <om:OMOBJ xmlns:om="http://www.openmath.org/OpenMath">
