<?xml version="1.0" encoding="UTF-8"?>
<omdoc xmlns:om="http://www.openmath.org/OpenMath" xmlns="http://omdoc.org/ns">
  <theory name="Distrib" base="http://latin.omdoc.org/math" meta="http://latin.omdoc.org/math?EqUniverse">
    <import name="mag1" from="http://latin.omdoc.org/math?Magma"> </import>
    <import name="mag2" from="http://latin.omdoc.org/math?Magma"> </import>
    <import name="right" from="http://latin.omdoc.org/math?RightDistrib">
      <import name="mag1" from="http://latin.omdoc.org/math?Magma">
        <definition>
          <om:OMS base="http://latin.omdoc.org/math" module="Distrib" name="mag1"/>
        </definition>
      </import>
      <import name="mag2" from="http://latin.omdoc.org/math?Magma">
        <definition>
          <om:OMS base="http://latin.omdoc.org/math" module="Distrib" name="mag2"/>
        </definition>
      </import>
    </import>
    <import name="left" from="http://latin.omdoc.org/math?RightDistrib">
      <import name="mag1" from="http://latin.omdoc.org/math?Magma">
        <definition>
          <om:OMS base="http://latin.omdoc.org/math" module="Distrib" name="mag1"/>
        </definition>
      </import>
      <import name="mag2" from="http://latin.omdoc.org/math?Magma">
        <definition>
          <om:OMA>
            <om:OMS position="0" base="http://cds.omdoc.org/mmt" module="mmt" name="composition"/>
            <om:OMS position="1" base="http://latin.omdoc.org/math" module="OppositeMagma"/>
            <om:OMS position="2" base="http://latin.omdoc.org/math" module="Distrib" name="mag2"/>
          </om:OMA>
        </definition>
      </import>
    </import>
